<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <AssemblyName>UnprocessedPackages</AssemblyName>
        <RootNamespace>UnprocessedPackages</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <Protobuf Include="Protos\unprocessed_packages.proto" GrpcServices="Server" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="AutoFixture" Version="4.18.0" />
        <PackageReference Include="Grpc.AspNetCore" Version="2.56.0" />
        <PackageReference Include="Grpc.AspNetCore.Server.Reflection" Version="2.56.0" />
    </ItemGroup>




</Project>
